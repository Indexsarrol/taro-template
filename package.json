{
  "name": "taro-template",
  "version": "1.0.0",
  "private": true,
  "description": "小程序taro模版",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "postinstall": "patch-package",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,scss}": "prettier --write",
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.4.13",
    "@tarojs/components": "3.4.13",
    "@tarojs/plugin-framework-react": "3.4.13",
    "@tarojs/react": "3.4.13",
    "@tarojs/runtime": "3.4.13",
    "@tarojs/taro": "3.4.13",
    "dayjs": "^1.11.3",
    "lodash": "4.17.15",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "taro-ui": "^3.1.0-beta.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@tarojs/mini-runner": "3.4.13",
    "@tarojs/webpack-runner": "3.4.13",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@umijs/fabric": "^2.12.2",
    "babel-preset-taro": "3.4.13",
    "eslint": "^8.12.0",
    "eslint-config-taro": "3.4.13",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.2.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.7.1",
    "sass-migrator": "^1.5.6"
  }
}
